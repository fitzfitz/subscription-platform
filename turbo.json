{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".wrangler/**", "dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "typecheck": {},
    "generate": {
      "cache": false
    },
    "migrate": {
      "cache": false
    },
    "migrate:local": {
      "cache": false
    },
    "seed:generate": {
      "cache": false
    },
    "test": {
      "dependsOn": ["build"]
    }
  }
}
